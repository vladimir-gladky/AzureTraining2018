{
  "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "testSAType": {
      "type": "string",
      "allowedValues": [
        "Standard_LRS",
        "Standard_ZRS",
        "Standard_GRS",
        "Standard_RAGRS",
        "Premium_LRS"
      ]
    }},
    "variables": {
        "testSAName": "[concat('sa', uniqueString(resourceGroup().id))]"
    },
  "resources": [
    {
      "name": "[variables('testSAName')]",
      "type": "Microsoft.Storage/storageAccounts",
      "location": "[resourceGroup().location]",
      "apiVersion": "2016-01-01",
      "sku": {
        "name": "[parameters('testSAType')]"
      },
      "dependsOn": [ ],
      "tags": {
        "displayName": "testSA"
      },
      "kind": "Storage"
    }],
  "outputs": {}
}